ruleset=Foreign,https://raw.githubusercontent.com/ACL4SSR/ACL4SSR/master/Clash/ProxyGFWlist.list

ruleset=OtherTraffic,[]FINAL

custom_proxy_group=Foreign`select`[]AutoSelect`[]Manual`[]Backup
custom_proxy_group=Manual`select`.*
custom_proxy_group=Backup`select`(Japan|JP|Singapore|SG)

custom_proxy_group=Domestic`select`[]DIRECT`[]Foreign`[]Manual
custom_proxy_group=AI`select`[]Backup
custom_proxy_group=OtherTraffic`select`[]Foreign`[]Manual

custom_proxy_group=AutoSelect`url-test`(^(?!.*(CT|CU|CM)).*)`http://www.gstatic.com/generate_204`300,,50

enable_rule_generator=true
overwrite_original_rules=true
